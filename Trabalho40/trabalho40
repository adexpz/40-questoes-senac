double cateto1, cateto2;

Console.Write("Digite o valor do cateto1: ");
cateto1 = Convert.ToDouble(Console.ReadLine());

Console.Write("Digite o valor do cateto2: ");
cateto2 = Convert.ToDouble(Console.ReadLine());

double hipotenusa = Math.Sqrt(cateto1 * cateto1 + cateto2 * cateto2);

Console.WriteLine($"O valor da hipotenusa Ã© {hipotenusa:F2}");
